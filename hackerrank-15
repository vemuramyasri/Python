#!/bin/python3

import math
import os
import random
import re
import sys
from collections import Counter


if __name__ == '__main__':
     s = input().strip()

     counter = Counter(s)

    # Sort by frequency descending, then alphabetically
     sorted_chars = sorted(counter.items(), key=lambda x: (-x[1], x[0]))

     for ch, cnt in sorted_chars[:3]:
        print(ch, cnt)
