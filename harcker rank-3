#!/bin/python3

import math
import os
import random
import re
import sys

from collections import Counter

if __name__ == '__main__':
    s = input()
    # 1. Count the occurrences of each character
    # Counter returns a dictionary-like object: {'a': 2, 'b': 3, 'c': 2, ...}
    counts = Counter(s)

    # 2. Sort the items
    # We sort by two criteria:
    # - Negative count (-x[1]) to get descending order for the frequency
    # - Character name (x[0]) to get ascending order for the alphabet
    sorted_chars = sorted(counts.items(), key=lambda x: (-x[1], x[0]))

    # 3. Print the top 3
    for i in range(3):
        print(f"{sorted_chars[i][0]} {sorted_chars[i][1]}")
