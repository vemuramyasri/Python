# Function definition 
def fibonacci(n): 
    if n <= 0: 
        return "Invalid Input" 
    elif n == 1: 
        return 0 
    elif n == 2: 
        return 1 
    else: 
        a, b = 0, 1 
        for i in range(2, n): 
            c = a + b 
            a = b 
            b = c 
        return b 
n = int(input("Enter the value of n: ")) 
print("The", n, "th Fibonacci number is:", fibonacci(n)) 
